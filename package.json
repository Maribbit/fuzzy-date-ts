{
  "name": "fuzzy-date-ts",
  "version": "0.2.0",
  "description": "A TypeScript library for handling dates with variable precision",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "test": "jest",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "date",
    "time",
    "fuzzy",
    "precision",
    "luxon",
    "typescript"
  ],
  "author": "maribbit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maribbit/fuzzy-date-ts.git"
  },
  "bugs": {
    "url": "https://github.com/maribbit/fuzzy-date-ts/issues"
  },
  "homepage": "https://github.com/maribbit/fuzzy-date-ts#readme",
  "files": [
    "dist/",
    "README.md",
    "README.zh-CN.md",
    "LICENSE"
  ],
  "dependencies": {
    "luxon": "^3.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  }
}
